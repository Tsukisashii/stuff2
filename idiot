setfpscap(6)

-- Unlock world
local args = {
	"UnlockWorld",
	"Seven Seas"
}
game:GetService("ReplicatedStorage")
    .Shared.Framework.Network.Remote.RemoteEvent:FireServer(unpack(args))

-- Load external script
loadstring(game:HttpGet("https://raw.githubusercontent.com/IdiotHub/Scripts/refs/heads/main/BGSI/main.lua"))()

-- Teleport player
local player = game:GetService("Players").LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
humanoidRootPart.CFrame = CFrame.new(-13945.51, 14.02, -20254.77)

-- Auto equip + sell loop
task.spawn(function()
    while task.wait(5) do -- every 5 seconds
        -- Equip best pets
        pcall(function()
            game:GetService("ReplicatedStorage").Shared.Framework.Network.Remote.RemoteEvent:FireServer("EquipBestPets")
        end)

        task.wait(1) -- small delay between calls

        -- Sell all fish
        pcall(function()
            game:GetService("ReplicatedStorage").Shared.Framework.Network.Remote.RemoteEvent:FireServer("SellAllFish")
        end)
    end
end)
