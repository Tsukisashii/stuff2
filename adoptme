setfpscap(5)
script_key="WorkFine";
getgenv().Config = {
    ["PetAMountToBuy"] = "999", 
    ["PetRemoteToBuy"] = "aztec_egg_2025_aztec_egg",

    ["PetFarmAutoSwitchFullGrown"] = true,
    ["PetFarmActive"] = false,
    ["LitePetFarmActive"] = true,
    ["EggFarmActive"] = true,

    ["HideUselessGui"] = true,
    ["Blur_username"] = false,
    ["Blazing_Lion_Log"] = false,
    ["DiscordId"] = "701390021898534912",
    ["Webhook"] = "https://discord.com/api/webhooks/1400470775592521798/-48oF_gPY30E3FhwJY8djz4k6eD64ZOgaWCmKHWzf9cW1nQN53WUoq0aOmd2JGWRJXWP",
}
loadstring(game:HttpGet("https://api.luarmor.net/files/v4/loaders/66567bfd337b57eb059b58dbe1badb89.lua"))()

-- Graphics optimization
if settings and settings().Rendering then
    settings().Rendering.QualityLevel = 1
    settings().Rendering.MeshCacheSize = 0
    settings().Rendering.EnableFRM = false
end

if game:GetService("RunService").IsStudio() == false then
    pcall(function()
        sethiddenproperty(game:GetService("Lighting"), "Technology", 2)
        sethiddenproperty(game:GetService("Lighting"), "GlobalShadows", false)
    end)
    game:GetService("Lighting").Brightness = 1
    game:GetService("Lighting").ClockTime = 14
    game:GetService("Lighting").GeographicLatitude = 0
    game:GetService("Lighting").Outlines = false
    game:GetService("Lighting").FogEnd = 100000
end

-- Remove all textures and visual effects
for _, v in pairs(game:GetDescendants()) do
    if v:IsA("Part") or v:IsA("MeshPart") or v:IsA("UnionOperation") then
        v.Material = Enum.Material.Plastic
        if v:FindFirstChildOfClass("Texture") then
            v:FindFirstChildOfClass("Texture"):Destroy()
        end
    elseif v:IsA("Decal") then
        v:Destroy()
    elseif v:IsA("ParticleEmitter") or v:IsA("Trail") or v:IsA("Beam") or v:IsA("Smoke") or v:IsA("Fire") or v:IsA("Sparkles") then
        v:Destroy()
    elseif v:IsA("SurfaceGui") or v:IsA("BillboardGui") then
        v:Destroy()
    elseif v:IsA("ShirtGraphic") then
        v:Destroy()
    elseif v:IsA("SpecialMesh") then
        v.TextureId = ""
    end
end
